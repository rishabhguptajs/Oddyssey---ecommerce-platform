{
  "name": "@nextui-org/input",
  "version": "2.1.16",
  "description": "The input component is designed for capturing user input within a text field.",
  "keywords": [
    "input"
  ],
  "author": "Junior Garcia <jrgarciadev@gmail.com>",
  "homepage": "https://nextui.org",
  "license": "MIT",
  "main": "dist/index.js",
